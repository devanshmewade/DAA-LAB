#include<stdio.h>

void read1(int arr[], int n) {
	printf("Enter %d elements : ",n);
	for (int i=0;i<n;i++) {
		scanf("%d",&arr[i]);
	}
}

int linearSearch (int arr[], int st, int end, int key) {
	if (st>=end) {
		return -1;
	}
	
	if (arr[st]==key) {
		return st;
	} 
		return linearSearch(arr, st+1, end, key);
	
}
